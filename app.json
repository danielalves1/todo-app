{
  "expo": {
    "name": "Todo App",
    "slug": "todo-app",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.daniel.alves1.todoapp",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.daniel.alves1.todoapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
